/PATMAX patArray length def
/freq 72 300 32 div div def
/sf {	/y exch def /x exch def
	/xindex x 1 add 2 div Bsize mul cvi def
	/yindex y 1 add 2 div Bsize mul cvi def
	/byte bitString yindex Bwid mul xindex 8 idiv add get def
	/mask 1 7 xindex 8 mod sub bitshift def
	byte mask and 0 ne
	{ /On On 1 add def 1 } { /Off Off 1 add def 0 } ifelse
} def
/sqpat {
	2 sub PATMAX mod
	patArray exch get dup dup
	2 get /angle exch def
	1 get /Bsize exch def
	0 get /bitString exch def
	/Bwid Bsize 8 idiv def
	/On 0 def /Off 0 def
	gsave
	freq angle /sf load setscreen
	{} settransfer
	Off Off On add div setgray
	fill
	grestore
} def	
/checkv {
	/DRIVER exch def
	DRIVER PREVERSION ne {
		/TR { /Times-Roman } def
		24 pt TR N
		600 1700 moveto
		(SoftQuad Publishing Software) show
		600 1600 moveto
		(Warning:) show
		14 pt TR N
		600 1520 moveto
		(Program sqdps expecting revision ) show
		DRIVER show
		( preamble.) show
		600 1450 moveto
		(Preamble is at revision ) show
		PREVERSION show
		(.) show
		600 1380 moveto
		/TB { /Times-Bold } def
		14 pt TB N
		(Attempting to continue processing) show
		showpage
		0 0 moveto
	} if
} def

% ensure pdfmark doesn't bomb out PS interpreters that lack it
/pdfmark where
{pop} {userdict /pdfmark /cleartomark load put} ifelse

%%EndProlog
%%BeginSetup
(3.2) checkv
(letter) 31 52 0 0 2400 3300 2550 3300 4 PTYPE
/TopOfPage 760 def	% used by psmerge
[/CropBox [45 93 509 TopOfPage] /PAGES pdfmark
%%EndSetup
