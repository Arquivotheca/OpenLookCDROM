XCOMM CCOPTIONS = -Wall -g -DNO_MENUS

INCLUDES = StandardIncludes -I. -I../include -I../canvas -Iparser

#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

SUBDIRS = parser

XCOMM Not currently used
TST_SRCS= test_client.c
TST_OBJS= test_client.o

/* All the autogenerated (someday - not yet) tty/process sources */
T_SRCS= tty.c process.c
T_OBJS= tty.o process.o

SRCS = Term.c handlers.c $(T_SRCS)
OBJS = Term.o handlers.o $(T_OBJS)

NormalLibraryTarget(term,$(OBJS))
DependTarget()

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
