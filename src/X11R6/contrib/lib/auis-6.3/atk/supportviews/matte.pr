#~group Matte

#~type boolean
#~views string boolean
#~help
When yes insets embedded in text can be resized, if no resizing insets is not allowed.
#~endhelp
ResizeInset: yes

#~type boolean
#~views string boolean
#~help
When yes insets embedded in text have a black border drawn around them, otherwise no border is drawn.
#~endhelp
DrawInsetBorder: no
