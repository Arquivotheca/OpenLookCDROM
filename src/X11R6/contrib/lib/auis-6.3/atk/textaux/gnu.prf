*.ClobberSymlinks: no
# tell ez to write to the file a symlink points to rather than overwriting the line with a new file
*.BackupExtension: ~
# save backup copies of a file, adding ~ to the end of the filename
*.RequireInitialFile: 0
*.ZipCreateTolerance: 0
*.ThumbScroll: yes
*.incsearchUseSelectionRegion: no
*.AlwaysDisplayStyleMenus: false
*.LineThruFormatNotes: yes
