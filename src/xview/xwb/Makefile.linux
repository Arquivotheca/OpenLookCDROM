# Imakefile for examples/frames

LOCATION = /usr/c-temp

DEFINES = -DSTANDALONE $(XVCLIENTSYSDEFINES)
SYS_LIBRARIES =
INCLUDES = -I. -I$(HEADER_DEST) -I$(TOP)
DEPLIBS =
LOCAL_LIBRARIES = $(XVIEWLIB) $(OLGXLIB) $(XLIB) $(XVCLIENTMISCLIB)

MAKEFILE_SIMPLE = Makefile.customer
SRCS = xwb.c
INSTALL_SRCS = $(SRCS)
ALLFILES = $(MAKEFILE_SIMPLE) $(SRCS)

PROGRAMS= xwb

all:: $(PROGRAMS)

xwb: xwb.o
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) xwb.o $(LOCAL_LIBRARIES) $(LDLIBS)   $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) xwb

# @(#)XView.prog	1.6 28 Jun 1993 SMI

populate::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	-@if [ -h SCCS ]; then \
	for i in $(ALLFILES) ; do sccs get $$i ; done ; \
	elif [ -h SCCS- ]; then \
	$(MV) SCCS- SCCS ; \
	for i in $(ALLFILES) ; do sccs get $$i ; done ; \
	$(MV) SCCS SCCS- ; \
	else \
	echo "SCCS or SCCS- do not exist" ; \
	fi

install::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	@for i in  $(SRC_DEST)/examples/bin; do if [ -d $(DESTDIR)$$i ]; then \
	set +x; else (set -x; $(MKDIRHIER) $(DESTDIR)$$i); fi \
	done

install:: $(PROGRAMS)
	@if [ -d $(DESTDIR)$(SRC_DEST)/examples/bin ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(SRC_DEST)/examples/bin); fi
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(PROGRAMS); do \
	(set -x; $(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) $$i $(DESTDIR)$(SRC_DEST)/examples/bin); \
	done

install.srcs::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	@for i in  $(SRC_DEST)/$(LOCATION); do if [ -d $(DESTDIR)$$i ]; then \
	set +x; else (set -x; $(MKDIRHIER) $(DESTDIR)$$i); fi \
	done

install.srcs::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	for i in $(INSTALL_SRCS) ;\
	do \
	echo "installing $$i"; \
	$(RM) $(SRC_DEST)/$(LOCATION)/$$i ; \
	$(INSTALL) -c $(INSTDATFLAGS) $$i $(SRC_DEST)/$(LOCATION) ; \
	done
	$(RM) $(SRC_DEST)/$(LOCATION)/Makefile ;
	$(INSTALL) -c $(INSTDATFLAGS) $(MAKEFILE_SIMPLE) $(SRC_DEST)/$(LOCATION)/Makefile

