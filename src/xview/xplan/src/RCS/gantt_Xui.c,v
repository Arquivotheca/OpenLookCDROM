@/*
 * gantt_ui.c - User interface object initialization functions.
 * This file was generated by `gxv' from `gantt.G'.
 * DO NOT EDIT BY HAND.
 */

#include <stdio.h>
#include <sys/param.h>
#include <sys/types.h>
#include <xview/xview.h>
#include <xview/canvas.h>
#include <xview/panel.h>
#include <xview/scrollbar.h>
#include <xview/svrimage.h>
#include <xview/termsw.h>
#include <xview/text.h>
#include <xview/tty.h>
#include <xview/xv_xrect.h>
#include "gantt_Xui.h"

gantt_ganttPopup_objects *Gantt_ganttPopup;

/*
 * Initialize an instance of object `ganttPopup'.
 */
gantt_ganttPopup_objects *
gantt_ganttPopup_objects_initialize(gantt_ganttPopup_objects *ip, Xv_opaque owner)
{
	if (!ip && !(ip = (gantt_ganttPopup_objects *) calloc(1, sizeof (gantt_ganttPopup_objects))))
		return (gantt_ganttPopup_objects *) NULL;
	if (!ip->ganttPopup)
		ip->ganttPopup = gantt_ganttPopup_ganttPopup_create(ip, owner);
	if (!ip->canvas)
		ip->canvas = gantt_ganttPopup_canvas_create(ip, ip->ganttPopup);
	return ip;
}

/*
 * Create object `ganttPopup' in the specified instance.
 */
Xv_opaque
gantt_ganttPopup_ganttPopup_create(gantt_ganttPopup_objects *ip, Xv_opaque owner)
{
	Xv_opaque	obj;
	
	obj = xv_create(owner, FRAME_CMD,
		XV_KEY_DATA, INSTANCE, ip,
		XV_WIDTH, 640,
		XV_HEIGHT, 480,
		XV_LABEL, "Gantt Chart",
		XV_SHOW, FALSE,
		FRAME_SHOW_FOOTER, TRUE,
		FRAME_SHOW_RESIZE_CORNER, TRUE,
		FRAME_CMD_PUSHPIN_IN, TRUE,
		NULL);
	xv_set(xv_get(obj, FRAME_CMD_PANEL), WIN_SHOW, FALSE, NULL);
	return obj;
}

/*
 * Create object `canvas' in the specified instance.
 */
Xv_opaque
gantt_ganttPopup_canvas_create(gantt_ganttPopup_objects *ip, Xv_opaque owner)
{
	extern void	gantt_repaint(Canvas, Xv_window, Display *, Window, Xv_xrectlist *);
	extern void	gantt_resize(Canvas, int, int);
	Xv_opaque	obj;
	
	obj = xv_create(owner, CANVAS,
		XV_KEY_DATA, INSTANCE, ip,
		XV_X, 0,
		XV_Y, 0,
		XV_WIDTH, WIN_EXTEND_TO_EDGE,
		XV_HEIGHT, WIN_EXTEND_TO_EDGE,
			CANVAS_HEIGHT, 32000,
			CANVAS_WIDTH, 32000,
			CANVAS_RETAINED, FALSE,
			CANVAS_AUTO_SHRINK, FALSE,
			CANVAS_AUTO_EXPAND, FALSE,
		CANVAS_REPAINT_PROC, gantt_repaint,
		CANVAS_X_PAINT_WINDOW, TRUE,
		CANVAS_RESIZE_PROC, gantt_resize,
		NULL);
	xv_create(obj, SCROLLBAR, SCROLLBAR_DIRECTION, SCROLLBAR_HORIZONTAL, NULL);
	xv_create(obj, SCROLLBAR, SCROLLBAR_DIRECTION, SCROLLBAR_VERTICAL, NULL);
	/*
	 * This line is here for backwards compatibility. It will be
	 * removed for the next release.
	 */
	xv_set(canvas_paint_window(obj), XV_KEY_DATA, INSTANCE, ip, NULL);
	return obj;
}

