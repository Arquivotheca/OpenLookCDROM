PERT_pertPopup_objects *PERT_pertPopup;

/*
 * Initialize an instance of object `pertPopup'.
 */
PERT_pertPopup_objects *
PERT_pertPopup_objects_initialize(PERT_pertPopup_objects *ip, Xv_opaque owner)
{
	if (!ip && !(ip = (PERT_pertPopup_objects *) calloc(1, sizeof (PERT_pertPopup_objects))))
		return (PERT_pertPopup_objects *) NULL;
	if (!ip->pertPopup)
		ip->pertPopup = PERT_pertPopup_pertPopup_create(ip, owner);
	if (!ip->canvas)
		ip->canvas = PERT_pertPopup_canvas_create(ip, ip->pertPopup);
	return ip;
}

/*
 * Create object `pertPopup' in the specified instance.
 */
Xv_opaque
PERT_pertPopup_pertPopup_create(PERT_pertPopup_objects *ip, Xv_opaque owner)
{
	Xv_opaque	obj;
	
	obj = xv_create(owner, FRAME_CMD,
		XV_KEY_DATA, INSTANCE, ip,
		XV_WIDTH, 640,
		XV_HEIGHT, 480,
		XV_LABEL, "PERT Chart",
		XV_SHOW, FALSE,
		FRAME_SHOW_FOOTER, TRUE,
		FRAME_SHOW_RESIZE_CORNER, TRUE,
		FRAME_CMD_PUSHPIN_IN, TRUE,
		NULL);
	xv_set(xv_get(obj, FRAME_CMD_PANEL), WIN_SHOW, FALSE, NULL);
	return obj;
}

/*
 * Create object `canvas' in the specified instance.
 */
Xv_opaque
PERT_pertPopup_canvas_create(PERT_pertPopup_objects *ip, Xv_opaque owner)
{
	extern void	PERT_Popup_repaint_callback(Canvas, Xv_window, Display *, Window, Xv_xrectlist *);
	extern void	PERT_Popup_resize_callback(Canvas, int, int);
	Xv_opaque	obj;
	
	obj = xv_create(owner, CANVAS,
		XV_KEY_DATA, INSTANCE, ip,
		XV_X, 0,
		XV_Y, 0,
		XV_WIDTH, WIN_EXTEND_TO_EDGE,
		XV_HEIGHT, WIN_EXTEND_TO_EDGE,
/* Added by Mark M. Lacey to get the scrollbars working */
			CANVAS_HEIGHT, 32000,
			CANVAS_WIDTH, 32000,
			CANVAS_RETAINED, FALSE,
			CANVAS_AUTO_SHRINK, FALSE,
			CANVAS_AUTO_EXPAND, FALSE,
		CANVAS_REPAINT_PROC, PERT_Popup_repaint_callback,
		CANVAS_X_PAINT_WINDOW, TRUE,
		CANVAS_RESIZE_PROC, PERT_Popup_resize_callback,
		NULL);

	xv_create(obj, SCROLLBAR, 
		  SCROLLBAR_DIRECTION, SCROLLBAR_HORIZONTAL,
		  SCROLLBAR_PIXELS_PER_UNIT, 40,
