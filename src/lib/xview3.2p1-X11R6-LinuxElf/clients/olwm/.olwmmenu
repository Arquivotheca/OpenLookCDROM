	    "Command Tool..."	$XVIEWHOME/bin/xview/cmdtool
	    "Mail Tool..."	$XVIEWHOME/bin/xview/mailtool
	    "Shell Tool..."	$XVIEWHOME/bin/xview/shelltool
	    "Tape Tool..."	tapetool
	    "Print Tool..."	printtool
	    "SunView Dbxtool..."	/usr/bin/dbxtool
