#define IHaveSubdirs 
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

SUBDIRS = examples 

DIRS_TO_CREATE = $(SRC_DEST) 

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
MakeDirectories(install, $(DIRS_TO_CREATE))
InstallSrcsSubdirs($(SUBDIRS))
PopulateSubdirs($(SUBDIRS))

