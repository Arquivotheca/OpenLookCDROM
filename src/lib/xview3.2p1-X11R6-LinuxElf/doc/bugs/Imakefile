#include <XView.tmpl>

/**/#########################################################################
/**/# @(#)Imakefile	1.9 28 Jun 1993 SMI
/**/# Imakefile for doc/bugs files.

DOCFILES = bugform

ALLFILES = $(DOCFILES) 

all::

install::
	$(MKDIRHIER) $(DOC_DEST) 

InstallNonExecList($(DOCFILES),$(DOC_DEST))
DependTarget()
LintTarget()
PopulateSCCSTree(h,$(ALLFILES))

clean::

depend::
