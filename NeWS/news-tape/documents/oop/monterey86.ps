 dup 8#224/rc put %right ceil
 dup 8#213/lt put %left  top curl
 dup 8#210/bv put %bold vert
 dup 8#217/lk put %left  mid curl
 dup 8#214/lb put %left  bot curl
 dup 8#215/rt put %right top curl
