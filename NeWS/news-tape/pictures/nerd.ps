%!
% Network extensibility Research & Development button
% Mike Gallaher, UniPress software
% Display with psview: "psview nerd.ps"

/nerdfont /Times-Roman findfont 50 scalefont def
/nerd {
    gsave
    currentpoint translate
    newpath
    0 0 90 0 360 arc stroke
    nerdfont setfont
    (NeRD) dup stringbbox 2 div neg exch 2 div neg exch
    moveto
    pop pop
    show stroke
    grestore
} def

/NerdMark {
    gsave
	0.12
	dup scale
	nerd
    grestore    
} def

erasepage
20 dup moveto NerdMark

100 100 moveto
3 { gsave
    3 { nerd 200 0 rmoveto pause
      } repeat
    grestore
    0 200 rmoveto
  } repeat

showpage
